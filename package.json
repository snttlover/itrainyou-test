{
  "name": "itrainyou",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production PORT=80 node .build/server.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
